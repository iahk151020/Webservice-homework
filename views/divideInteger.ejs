<H1>Devide 2 integer numbers</H1>
<form action="/integer/divideInteger" method="POST">
    <div class="mb-3">
      <label for="exampleInputEmail1" class="form-label">argument 1</label>
      <input type="number" class="form-control" name="arg1" id="arg1">
    </div>
    <div class="mb-3">
      <label for="exampleInputPassword1" class="form-label">argument 2</label>
      <input type="number" class="form-control" name="arg2" id="arg21">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<% if (result != null) { %>
    <br>
    <div class="card">
        <div class="card-body">
          Result: <%= result %>
        </div>
      </div>
<% } %>
